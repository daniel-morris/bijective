{
    "name": "honest/bijective",
    "description": "Bijective is a class that can compute pairings between alphanumeric strings, and integers. Every integer can be mapped to an alphanumeric string, and every alphanumeric string can be mapped back to an integer with no unpaired permutations.",
    "type": "Library",
    "homepage": "https://github.com/daniel-morris/bijective",
    "keywords": ["bijective", "bijection"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Morris",
            "email": "daniel@honestempire.com",
            "homepage": "https://github.com/daniel-morris",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/daniel-morris/bijective"
        }
    ],
    "version": "1.0.6",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7.0",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-0": {
            "Honest": "src"
        }
    }
}
